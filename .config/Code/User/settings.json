    "**/.mypy_cache": true,
    "**/.pytest_cache": true,
    "**/__pycache__": true
  },
  "files.hotExit": "onExit",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "githubPullRequests.telemetry.enabled": false,
  "gitlens.advanced.telemetry.enabled": false,
  "gitlens.blame.highlight.locations": [
    "overview",
    "gutter",
    "line"
