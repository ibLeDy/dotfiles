{
    "[python]": {
        "editor.defaultFormatter": "ms-python.python",
        "editor.semanticHighlighting.enabled": false,
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features",
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.tabSize": 2,
        "editor.formatOnSave": true,
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features",
        "editor.tabSize": 2,
        "editor.formatOnSave": true,
    },
    "[css]": {
        "editor.defaultFormatter": "vscode.css-language-features",
        "editor.tabSize": 2,
        "editor.formatOnSave": true,
    },
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml",
        "editor.formatOnSave": true,
    },
    "debug.console.fontSize": 13,
    "discord.lowerDetailsNoWorkspaceFound": "{empty}",
    "discord.lowerDetailsIdling": "{empty}",
    "discord.largeImage": "{lang}",
    "discord.detailsEditing": "Editing {file_name}:{current_line}",
    "discord.lowerDetailsEditing": "Workspace: {workspace}:{git_branch}",
    "discord.largeImageIdling": "AFK",
    "discord.lowerDetailsDebugging": "Workspace: {workspace}:{git_branch}",
    "editor.acceptSuggestionOnEnter": "off",
    "editor.cursorBlinking": "solid",
    "editor.cursorSmoothCaretAnimation": true,
    "editor.detectIndentation": true,
    "editor.dragAndDrop": false,
    "editor.emptySelectionClipboard": true,
    "editor.find.seedSearchStringFromSelection": true,
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": false,
    "editor.mouseWheelScrollSensitivity": 1,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.quickSuggestionsDelay": 50,
    "editor.showFoldingControls": "mouseover",
    "editor.smoothScrolling": true,
    "editor.snippetSuggestions": "top",
    "editor.suggest.shareSuggestSelections": true,
    "editor.suggest.showStatusBar": true,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "on",
    "editor.tabSize": 4,
    "editor.trimAutoWhitespace": true,
    "editor.useTabStops": true,
    "editor.wordSeparators": "./\\()\"'-:,.;<>~!@#$%^&*|+=[]{}`~?",
    "editor.wrappingIndent": "deepIndent",
    "errorLens.addNumberOfDiagnostics": true,
    "explorer.confirmDragAndDrop": false,
    "explorer.autoReveal": true,
    "extensions.autoUpdate": false,
    "search.exclude": {
        "**/.mypy_cache": true,
        "**/.pytest_cache": true,
        "**/*.egg-info": true,
        "**/*.pyc": true,
        "**/*venv": true,
        "**/__pycache__": true,
    },
    "files.autoSave": "off",
    "files.encoding": "utf8",
    "files.exclude": {
        "**/.mypy_cache": true,
        "**/.pytest_cache": true,
        "**/*.egg-info": true,
        "**/*.pyc": true,
        "**/*venv": true,
        "**/__pycache__": true,
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "files.hotExit": "onExit",
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true,
    "githubPullRequests.telemetry.enabled": false,
    "gitlens.blame.highlight.locations": [
        "overview",
        "gutter",
        "line",
    ],
    "gitlens.codeLens.authors.enabled": false,
    "jupyter.sendSelectionToInteractiveWindow": false,
    "markdownlint.config": {
        "MD033": false,
        "MD041": false,
        "MD007": false,
    },
    "oneDarkPro.editorTheme": "vscode",
    "partialDiff.enableTelemetry": false,
    "python.analysis.autoImportCompletions": false,
    "python.analysis.extraPaths": [
        "./src"
    ],
    "python.analysis.useLibraryCodeForTypes": true,
    "python.autoComplete.extraPaths": [
        "./src"
    ],
    "python.diagnostics.sourceMapsEnabled": true,
    "python.disableInstallationCheck": true,
    "python.experiments.enabled": false,
    "python.formatting.blackArgs": [
        "--skip-string-normalization"
    ],
    "python.languageServer": "Pylance",
    "python.linting.flake8Args": [
        "--max-line-length=88",
        "--ignore=E501"
    ],
    "python.linting.flake8CategorySeverity.F": "Information",
    "python.linting.flake8Enabled": true,
    "python.linting.maxNumberOfProblems": 1000,
    "python.linting.mypyArgs": [
        "--disallow-incomplete-defs",
        "--disallow-any-generics",
        "--check-untyped-defs",
        "--no-implicit-optional",
        "--no-implicit-reexport",
        "--show-error-codes",
        "--warn-redundant-casts",
        "--warn-unused-ignores",
        "--warn-unused-configs",
        "--warn-return-any",
        "--warn-unreachable",
        "--follow-imports=silent",
        "--ignore-missing-imports",
    ],
    "python.showStartPage": false,
    "python.venvPath": "~/.virtualenvs",
    "scm.alwaysShowRepositories": true,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "terminal.integrated.fontFamily": "UbuntuMono Nerd Font",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.enableBell": true,

    "terminal.integrated.profiles.windows": {
        "bash": {
            "source": "Git Bash",
            "icon": "terminal-bash",
        }
    },
    "terminal.integrated.defaultProfile.windows": "bash",
    "terminal.integrated.profiles.linux": {
        "bash": {
            "path": "/usr/bin/bash",
            "icon": "terminal-bash",
        },
        "zsh": {
            "path": "/usr/bin/zsh",
            "icon": "terminal-linux",
        },
        "tmux": {
            "path": "/usr/local/bin/tmux",
            "icon": "terminal-tmux"
        },
    },
    "terminal.integrated.defaultProfile.linux": "zsh",
    "terminal.integrated.profiles.osx": {
        "bash": {
            "path": "bash",
            "icon": "terminal-bash"
        },
        "zsh": {
            "path": "/bin/zsh",
            "icon": "terminal-linux",
        },
        "tmux": {
            "path": "tmux",
            "icon": "terminal-tmux"
        },
    },
    "terminal.integrated.defaultProfile.osx": "zsh",
    "terminal.external.windowsExec": "C:\\Program Files\\Alacritty\\alacritty.exe",
    "update.mode": "start",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "vsintellicode.features.python.deepLearning": "enabled",
    "window.closeWhenEmpty": false,
    "window.menuBarVisibility": "toggle",
    "window.restoreFullscreen": false,
    "window.title": "${dirty}${activeEditorMedium}${separator}${rootName} ${dirty}",
    "window.titleBarStyle": "custom",
    "workbench.colorTheme": "One Dark Pro",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.pinnedTabSizing": "shrink",
    "workbench.editor.tabCloseButton": "right",
    "workbench.editorAssociations": [
        {
            "viewType": "jupyter.notebook.ipynb",
            "filenamePattern": "*.ipynb"
        }
    ],
    "workbench.enableExperiments": false,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.list.smoothScrolling": true,
    "workbench.statusBar.visible": true,
    "workbench.preferredDarkColorTheme": "Sublime Material Theme - Dark",
    "workbench.tree.indent": 22,
    "python.linting.enabled": true,
    "python.linting.pylintUseMinimalCheckers": false,
    "xmlTools.enforcePrettySelfClosingTagOnFormat": true,
    "xmlTools.enableXmlTreeView": false,
    // "workbench.colorCustomizations": {
    //     "editorCursor.foreground": "#0051ff",
    // },
    "editor.fontSize": 13,
    "window.zoomLevel": 0,
    "gitlens.codeLens.enabled": false,
    "rust-analyzer.cargo.allFeatures": true,
    "rust-analyzer.lens.enable": false,
    "terminal.external.linuxExec": "gnome-terminal",
    "python.linting.mypyEnabled": true,
    "workbench.editor.openPositioning": "last",
    "editor.rulers": [
        88,
        120,
    ],
    "workbench.editor.decorations.badges": true,
    "workbench.editor.decorations.colors": true,
    "workbench.editor.enablePreviewFromCodeNavigation": true,
    "oneDarkPro.markdownStyle": false,
    "python.analysis.typeCheckingMode": "basic",
    "editor.occurrencesHighlight": true,
    "bracket-pair-colorizer-2.showBracketsInGutter": true,
    "bracket-pair-colorizer-2.showBracketsInRuler": true,
    "bracket-pair-colorizer-2.colorMode": "Independent",
    "python.formatting.provider": "black",
    "python.formatting.yapfArgs": [
        "--style=google"
    ],
    "workbench.list.horizontalScrolling": true,
    "workbench.tree.expandMode": "doubleClick",
    "workbench.sash.size": 6,
    "editor.selectionClipboard": false,
    "search.smartCase": true,
    "settingsSync.ignoredSettings": [
        "python.pythonPath"
    ],
    "editor.linkedEditing": true,
    "workbench.sash.hoverDelay": 0,
    "workbench.hover.delay": 250,
    "python.pythonPath": "python3",
    "html.format.endWithNewline": true,
    "html.format.indentInnerHtml": true,
    "files.associations": {
        "**/*.html": "html",
        "**/templates/**/*.html": "html, django-html",
        "**/templates/**/*": "django-txt",
        "**/requirements{/**,*}.{txt,in}": "pip-requirements"
    },
    "emmet.includeLanguages": {
        "django-html": "html"
    },
    "beautify.language": {
        "html": [
            "htm",
            "html",
            "django-html"
        ]
    },
    "django.i18n": false,
    "html-css-class-completion.enableEmmetSupport": true,
    "html.format.templating": true,
    "docker.showStartPage": false,
    "workbench.productIconTheme": "fluent-icons",
    "prettier.vueIndentScriptAndStyle": true,
    "html.format.wrapAttributes": "aligned-multiple",
    "redhat.telemetry.enabled": false,
    "explorer.sortOrder": "filesFirst",
    "editor.minimap.showSlider": "always",
    "html.format.indentHandlebars": true,
    "[django-html]": {
        "editor.defaultFormatter": "HookyQR.beautify",
        "editor.formatOnSave": true
    },
}
