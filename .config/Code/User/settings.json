{
    "[python]": {
        "editor.defaultFormatter": "ms-python.python",
        "editor.semanticHighlighting.enabled": false,
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features",
    },
    "bracketPairColorizer.showBracketsInGutter": true,
    "bracketPairColorizer.showBracketsInRuler": true,
    "debug.console.fontSize": 13,
    "discord.detailsEditing": "File: {filename}:{currentline}",
    "discord.largeImageIdle": "\"I was taking a shit\"",
    "discord.lowerDetailsDebugging": "Debugging {workspace}",
    "discord.lowerDetailsEditing": "Project: {workspace}:{gitbranch}",
    "discord.lowerDetailsIdle": "{null}",
    "discord.lowerDetailsNotFound": "Total lines: {totallines}",
    "discord.workspaceElapsedTime": true,
    "editor.acceptSuggestionOnEnter": "off",
    "editor.cursorBlinking": "solid",
    "editor.cursorSmoothCaretAnimation": true,
    "editor.detectIndentation": true,
    "editor.dragAndDrop": false,
    "editor.emptySelectionClipboard": true,
    "editor.find.addExtraSpaceOnTop": false,
    "editor.find.seedSearchStringFromSelection": true,
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "editor.formatOnPaste": true,
    "editor.formatOnType": true,
    "editor.largeFileOptimizations": false,
    "editor.minimap.showSlider": "always",
    "editor.mouseWheelScrollSensitivity": 1,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.occurrencesHighlight": false,
    "editor.quickSuggestionsDelay": 50,
    "editor.selectionClipboard": false,
    "editor.showFoldingControls": "mouseover",
    "editor.smoothScrolling": true,
    "editor.snippetSuggestions": "top",
    "editor.suggest.shareSuggestSelections": true,
    "editor.suggest.showStatusBar": true,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "on",
    "editor.tabSize": 4,
    "editor.trimAutoWhitespace": true,
    "editor.useTabStops": true,
    "editor.wordSeparators": "./\\()\"'-:,.;<>~!@#$%^&*|+=[]{}`~?",
    "editor.wrappingIndent": "deepIndent",
    "errorLens.addNumberOfDiagnostics": true,
    "explorer.confirmDragAndDrop": false,
    "explorer.autoReveal": true,
    "extensions.autoUpdate": false,
    "files.associations": {
        "**/*.html": "html",
        "**/requirements{/**,*}.{txt,in}": "pip-requirements",
        "**/templates/**/*": "django-txt",
        "**/templates/**/*.html": "django-html",
        "**/*.txt": "plaintext",
    },
    "search.exclude": {
        "**/.mypy_cache": true,
        "**/.pytest_cache": true,
        "**/*.pyc": true,
        "**/*venv": true,
        "**/__pycache__": true,
    },
    "files.autoSave": "off",
    "files.encoding": "utf8",
    "files.exclude": {
        "**/.mypy_cache": true,
        "**/.pytest_cache": true,
        "**/*.pyc": true,
        "**/*venv": true,
        "**/*.egg-info": true,
        "**/__pycache__": true,
    },
    "files.hotExit": "onExit",
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true,
    "githubPullRequests.telemetry.enabled": false,
    "gitlens.blame.highlight.locations": [
        "overview",
        "gutter",
        "line",
    ],
    "gitlens.codeLens.authors.enabled": false,
    "jupyter.sendSelectionToInteractiveWindow": false,
    "markdownlint.config": {
        "MD033": false,
        "MD041": false,
    },
    "oneDarkPro.editorTheme": "vscode",
    "partialDiff.enableTelemetry": false,
    "python.analysis.autoImportCompletions": false,
    "python.analysis.extraPaths": [
        "./src"
    ],
    "python.analysis.useLibraryCodeForTypes": true,
    "python.autoComplete.extraPaths": [
        "./src"
    ],
    "python.diagnostics.sourceMapsEnabled": true,
    "python.disableInstallationCheck": true,
    "python.experiments.enabled": false,
    "python.formatting.blackArgs": [
        "--skip-string-normalization"
    ],
    "python.formatting.provider": "none",
    "python.languageServer": "Pylance",
    "python.linting.flake8Args": [
        "--max-line-length=88",
        "--ignore=E501"
    ],
    "python.linting.flake8CategorySeverity.F": "Information",
    "python.linting.flake8Enabled": true,
    "python.linting.maxNumberOfProblems": 1000,
    "python.linting.mypyArgs": [
        "--no-implicit-reexport",
        "--warn-redundant-casts",
        "--warn-unused-ignores",
        "--no-implicit-optional",
        "--disallow-incomplete-defs",
        "--check-untyped-defs",
        "--warn-unused-configs",
        "--warn-return-any",
        "--warn-unreachable",
        "--show-error-codes",
        "--disallow-any-generics",
        "--follow-imports=silent",
        "--ignore-missing-imports",
        "--disable-error-code=var-annotated"
    ],
    "python.pythonPath": "/usr/bin/python3",
    "python.showStartPage": false,
    "python.venvPath": "~/.virtualenvs",
    "scm.alwaysShowRepositories": true,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "terminal.integrated.fontFamily": "UbuntuMono Nerd Font",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.enableBell": true,
    "terminal.integrated.shell.linux": "/usr/bin/zsh",
    "terminal.integrated.shell.osx": "/bin/zsh",
    "terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
    "terminal.external.windowsExec": "C:\\Program Files\\Alacritty\\alacritty.exe",
    "update.mode": "start",
    "vsicons.dontShowNewVersionMessage": true,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "vsintellicode.features.python.deepLearning": "enabled",
    "window.closeWhenEmpty": false,
    "window.menuBarVisibility": "toggle",
    "window.restoreFullscreen": false,
    "window.title": "${dirty}${activeEditorMedium}${separator}${rootName} ${dirty}",
    "window.titleBarStyle": "custom",
    "workbench.activityBar.visible": true,
    "workbench.colorTheme": "One Dark Pro",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.pinnedTabSizing": "shrink",
    "workbench.editor.tabCloseButton": "right",
    "workbench.editorAssociations": [],
    "workbench.enableExperiments": false,
    "workbench.iconTheme": "vscode-icons",
    "workbench.list.smoothScrolling": true,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.statusBar.visible": true,
    "python.analysis.memory.keepLibraryAst": true,
    "workbench.preferredDarkColorTheme": "Sublime Material Theme - Dark",
    "workbench.tree.indent": 21,
    "python.linting.enabled": true,
    "python.linting.pylintUseMinimalCheckers": false,
    "xmlTools.enforcePrettySelfClosingTagOnFormat": true,
    "xmlTools.enableXmlTreeView": false,
    "workbench.colorCustomizations": {
        "sash.hoverBorder": "#4B8BBE"
    },
    "editor.fontSize": 13,
    "window.zoomLevel": 0,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "gitlens.codeLens.enabled": false,
    "rust-analyzer.cargo.allFeatures": true,
    "rust-analyzer.lens.enable": false,
    "terminal.external.linuxExec": "gnome-terminal",
    "python.linting.mypyEnabled": true,
    "workbench.editor.openPositioning": "last",
    "editor.rulers": [
        88,
    ],
    "workbench.editor.wrapTabs": false,
    "workbench.editor.decorations.badges": true,
    "workbench.editor.decorations.colors": true,
    "workbench.editor.enablePreviewFromCodeNavigation": true,
    "RPC.idleTimeout": 600,
    "RPC.showProblems": false,
    "RPC.placeholderText": "VSCode",
    "oneDarkPro.markdownStyle": false,
    "RPC.disconnectOnIdle": true,
    "python.analysis.typeCheckingMode": "basic",
}
